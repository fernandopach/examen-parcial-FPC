<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>elementos-yo-alumno</display-name>
    <servlet>
        <servlet-name>ServletHoroscopo</servlet-name>
        <servlet-class>com.jc.elementos.controller.ServletHoroscopo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletPagarNomina</servlet-name>
        <servlet-class>com.jc.elementos.controller.ServletPagarNomina</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletBorrarNomina</servlet-name>
        <servlet-class>com.jc.elementos.controller.ServletBorrarNomina</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletExamen</servlet-name>
        <servlet-class>com.jc.elementos.controller.ServletExamen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletExamen</servlet-name>
        <servlet-class>com.jc.elementos.controller.ServletExamen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>clientee</servlet-name>
        <servlet-class>com.jc.elementos.model.clientee</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletCliente</servlet-name>
        <servlet-class>com.jc.elementos.controller.ServletCliente</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletHoroscopo</servlet-name>
        <url-pattern>/horoscopo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletPagarNomina</servlet-name>
        <url-pattern>/nomina</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletBorrarNomina</servlet-name>
        <url-pattern>/borrar-nomina</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletExamen</servlet-name>
        <url-pattern>/ServletExamen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletExamen</servlet-name>
        <url-pattern>/resultados</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>clientee</servlet-name>
        <url-pattern>/clientee</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletCliente</servlet-name>
        <url-pattern>/ServletCliente</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
